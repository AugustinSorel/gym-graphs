{
  "name": "@gym-graphs/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    },
    "./db": {
      "types": "./dist/db/db.schemas.d.ts"
    }
  },
  "scripts": {
    "dev": "tsx watch ./src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "db:bash": "docker exec -it gym_graphs_db bash -c 'psql -U postgres -d gym_graphs'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.726.1",
    "@gym-graphs/constants": "workspace:*",
    "@gym-graphs/schemas": "workspace:*",
    "@hono/node-server": "^1.19.5",
    "@hono/zod-validator": "^0.7.4",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.39.1",
    "hono": "^4.9.11",
    "pg": "^8.13.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/pg": "^8.11.10",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6"
  }
}
