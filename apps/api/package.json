{
  "name": "@gym-graphs/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    },
    "./db": {
      "types": "./dist/db/db.schemas.d.ts"
    }
  },
  "scripts": {
    "build:types": "tsc",
    "dev": "bun --watch ./src/index.ts",
    "db:bash": "docker exec -it gym_graphs_db bash -c 'psql -U postgres -d gym_graphs'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.726.1",
    "@gym-graphs/schemas": "workspace:*",
    "@gym-graphs/constants": "workspace:*",
    "@hono/zod-validator": "^0.7.4",
    "drizzle-orm": "^0.39.1",
    "hono": "^4.9.11",
    "pg": "^8.13.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.30.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
